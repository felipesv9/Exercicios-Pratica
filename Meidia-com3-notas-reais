#include<stdio.h>
#include<stdlib.h>

float main()
{
    float nota1,nota2,nota3, media;
    printf("digite sua nota1: ");
    scanf("%f", &nota1);
    
    printf("digite sua nota2: ");
    scanf("%f", &nota2);
    
    printf("digite sua nota3: ");
    scanf("%f", &nota3);
    
    media=(nota1+nota2+nota3)/3;
    printf("sua media e; %f", media);
    
    return 0;
}
